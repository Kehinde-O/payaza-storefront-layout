{
  "name": "payaza-storefront-layouts",
  "version": "1.0.2",
  "description": "Shared layout components for StoreFront applications",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./json": {
      "types": "./dist/json/json-map.d.ts",
      "import": "./dist/json/json-map.js",
      "require": "./dist/json/json-map.js",
      "default": "./dist/json/json-map.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && (tsc --noEmitOnError false || true) && tsc-alias && npm run copy-json",
    "copy-json": "mkdir -p dist/json && cp -r src/json/*.json dist/json/ 2>/dev/null || true",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "clean": "rm -rf dist",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "peerDependencies": {
    "framer-motion": "^12.0.0",
    "next": "^16.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "peerDependenciesMeta": {
    "framer-motion": {
      "optional": false
    },
    "next": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@types/node": "^20",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.28",
    "lucide-react": "^0.554.0",
    "nprogress": "^0.2.0",
    "tailwind-merge": "^3.4.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5"
  },
  "keywords": [
    "storefront",
    "layouts",
    "react",
    "nextjs",
    "components",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

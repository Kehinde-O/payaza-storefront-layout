{
  "name": "payaza-storefront-layouts",
  "version": "1.0.59",
  "type": "module",
  "description": "Shared layout components for StoreFront applications",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./json": {
      "types": "./dist/json/json-map.d.ts",
      "import": "./dist/json/json-map.js",
      "require": "./dist/json/json-map.js",
      "default": "./dist/json/json-map.js"
    },
    "./requirements": {
      "types": "./dist/requirements/requirements-map.d.ts",
      "import": "./dist/requirements/requirements-map.js",
      "require": "./dist/requirements/requirements-map.js",
      "default": "./dist/requirements/requirements-map.js"
    },
    "./editor": {
      "types": "./dist/editor/index.d.ts",
      "import": "./dist/editor/index.js",
      "require": "./dist/editor/index.js",
      "default": "./dist/editor/index.js"
    },
    "./editor/puck": {
      "types": "./dist/editor/puck/index.d.ts",
      "import": "./dist/editor/puck/index.js",
      "require": "./dist/editor/puck/index.js",
      "default": "./dist/editor/puck/index.js"
    },
    "./editor/rjsf": {
      "types": "./dist/editor/rjsf/index.d.ts",
      "import": "./dist/editor/rjsf/index.js",
      "require": "./dist/editor/rjsf/index.js",
      "default": "./dist/editor/rjsf/index.js"
    },
    "./styles": "./dist/styles/index.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && (tsc --noEmitOnError false || true) && tsc-alias && npm run copy-json && npm run build-css",
    "build-css": "mkdir -p dist/styles && postcss src/styles/index.css -o dist/styles/index.css",
    "copy-json": "mkdir -p dist/json && cp -r src/json/*.json dist/json/ 2>/dev/null || true",
    "watch": "npm run clean && npm run copy-json && npm run build-css && npm run watch:all",
    "watch:all": "npm-run-all --parallel watch:ts watch:css watch:json",
    "watch:ts": "node scripts/watch-ts.cjs",
    "watch:css": "postcss src/styles/index.css -o dist/styles/index.css --watch",
    "watch:json": "node scripts/watch-json.cjs",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "clean": "rm -rf dist",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "peerDependencies": {
    "@rjsf/core": "^6.1.2",
    "@rjsf/utils": "^6.1.2",
    "@rjsf/validator-ajv8": "^6.1.2",
    "@tailwindcss/postcss": "^4.0.0",
    "framer-motion": "^12.0.0",
    "next": "^16.0.3",
    "postcss": "^8.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@rjsf/core": {
      "optional": true
    },
    "@rjsf/utils": {
      "optional": true
    },
    "@rjsf/validator-ajv8": {
      "optional": true
    },
    "@tailwindcss/postcss": {
      "optional": false
    },
    "framer-motion": {
      "optional": false
    },
    "next": {
      "optional": false
    },
    "postcss": {
      "optional": false
    },
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    },
    "tailwindcss": {
      "optional": false
    }
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@rjsf/shadcn": "^6.1.2",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "nprogress": "^0.2.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@rjsf/core": "^6.1.2",
    "@rjsf/utils": "^6.1.2",
    "@rjsf/validator-ajv8": "^6.1.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-jsx": "^2.2.9",
    "framer-motion": "^12.25.0",
    "next": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.0",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^4.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5"
  },
  "keywords": [
    "storefront",
    "layouts",
    "react",
    "nextjs",
    "components",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}